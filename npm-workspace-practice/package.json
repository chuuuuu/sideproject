{
  "name": "nwp",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "npm run dev --workspace=packages/server",
    "dev:next": "npm run dev --workspace=packages/next",
    "dev:web": "npm run start --workspace=packages/web",
    "dev:app": "npm run start --workspace=packages/app",
